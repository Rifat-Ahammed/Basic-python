   "source": [
    "num = int(input(\"Number for summation: \"))\n",
    "\n",
    "sum = 0\n",
    "\n",
    "i = 1\n",
    "while(i <= num):\n",
    "    if(i % 2 == 0):\n",
    "        sum = sum + i\n",
    "    i = i + 1\n",
    "    \n",
    "print(f\"Summation of even number is: {sum}\")"
   ]
